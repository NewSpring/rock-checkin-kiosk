{
   "name":"KidSpring Check-in",
   "version":"0.1",
   "manifest_version":2,
   "minimum_chrome_version":"37.0.0.0",
   "permissions":[
      "webview",
      "power",
      "pointerLock",
      "system.display"
   ],
   "app":{
      "background":{
         "scripts":[
            "js/foam.js",
            "js/cab.js",
            "config.js",
            "background_main.js"
         ]
      }
   },
   "kiosk_enabled":true,
   "default_locale":"en",
   "icons":{
      "128":"img/logo.png"
   }
}
